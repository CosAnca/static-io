doctype html
html(lang="en")
	head
		title= title
		include ../includes/head/_meta
		include ../includes/head/_favicons
		// Stylesheet links
		link(rel="stylesheet", href="#{baseUrl}css/style.css")
	body

		//- To use your defined Jade mixins include the mixins file below
		include ../mixins/_mixins

		//- This is where you include your header
		include _header

		//- This is where your block content starts
		block content

		//- This is where you include your footer
		include _footer

		//- This is where you include your scripts
		block scripts
			include ../includes/scripts/_scripts
	
